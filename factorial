import java.util.*;
public class sum_upto_n {

	public static void main(String[] args) {
		
		System.out.println("Enter the number");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int i = 1;
		int sum = 1;
		while (i <= n) {
			
			sum = sum * i;
			i++;
		}
		
			System.out.println("Sum of numbers :" + "   " + sum);
	}

}
