public class MyClass {
    public static void main(String args[]) {
      	int row =5 ;
		int i = 0,j=0;
		for (i=1; i<=row; i++)
        {
            for ( j=1; j<=row; j++)
            {
                if(i ==1 || i == row   || j == 1 || j == row )
                    System.out.print("*");
                else
                    System.out.print(" ");
            }
            System.out.println();
		            
		        }
}
}




