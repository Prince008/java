import java.util.*;
public class multiplication_Table {

	public static void main(String[] args) {


		int n, i;
		
		System.out.println("Enter the number : ");
		Scanner sc = new Scanner(System.in);
		n = sc.nextInt();
		
		for(i = 1 ; i <= 10 ; i++) {
			
			System.out.println(n + "*" + i +  "=" + (n*i));
		}
			

	}

}
